{
    "dados_cat":{
        "versao":1
    },
    "drivers":{
        "oracle":{
            "name":"oracle",
            "tables":{
                "tables":{
                    "name":"all_tables",
                    "columns":{
                        "schema_name":"owner",
                        "table_name":"table_name"
                    }
                }
            },
            "strings":{
                "strdataandroid":"yyyy-mm-dd hh24:mi:ss"
            },
            "functions":{
                "lower":"lower",
                "current_date":"sysdate",
                "current_datetime":"sysdate"
            },
            "statements":{
                "table_exists":"select count(1) as existe from all_tables where lower(owner) = lower(:database_name) and lower(table_name) = lower(:table_name)"
            },
            "types":{
                "numeric":{
                    "name":"number",
                    "synonyms":["number","numeric","numero","float","real","double"]
                },
                "varchar":{
                    "name":"varchar2",
                    "synonyms":["varchar","varchar2","string","text","texto"]
                },
                "blob":{
                    "name":"clob",
                    "synonyms":["blob","clob","text","longtext"]
                },
                "datetime":{
                    "name":"date",
                    "synonyms":["datetime","sysdate","date"]
                },
                "bit":{
                    "name":"number",
                    "synonyms":["bit","byte"]
                }
            }
        },
        "mysql":{
            "name":"mysql",
            "tables":{
                "tables":{
                    "name":"information_schema.tables",
                    "columns":{
                        "schema_name":"table_schema",
                        "table_name":"table_name"
                    }
                }
            },
            "strings":{
                "delimiter":"'"
            },
            "functions":{
                "lower":"lower",
                "current_date":"current_date",
                "current_datetime":"current_timestamp"
            },
            "statements":{
                "table_exists":"select count(1) as existe from information_schema.tables where lower(table_schema) = lower(:database_name) and lower(table_name) = lower(:table_name)"
            },
            "types":{
                "numeric":{
                    "name":"numeric",
                    "synonyms":["number","numeric","numero","float","real","double"]
                },
                "varchar":{
                    "name":"varchar",
                    "synonyms":["varchar","varchar2","string","text","texto"]
                },
                "blob":{
                    "name":"longtext",
                    "synonyms":["blob","clob","text","longtext"]
                },
                "datetime":{
                    "name":"datetime",
                    "synonyms":["datetime","sysdate"]
                }
            }
        }
    },
    "hosts":{
        "localhost":{
            "host":"localhost",
            "allow_ddl":true
        },
        "winthor":{
            "host":"192.168.2.42",
            "allow_ddl":true
        }
    },
    "schemas":{
        "sjd":{
            "name":"sjd",
            "host":{"$ref":"#/hosts/localhost"},
            "allow_ddl":true
        },
        "consulta":{
            "name":"consulta",
            "host":{"$ref":"#/hosts/winthor"},
            "allow_ddl":true
        },
        "consulta2":{
            "name":"consulta2",
            "host":{"$ref":"#/hosts/winthor"},
            "allow_ddl":true
        },
        "jumbo":{
            "name":"jumbo",
            "host":{"$ref":"#/hosts/winthor"},
            "allow_ddl":false
        }
    },
    "connections":{
        "mysql_sjd":{
            "name":"mysql_sjd",
            "driver":{"$ref":"#/drivers/mysql"},
            "host":{"$ref":"#/hosts/localhost"},
            "port":3306,
            "user":"root",
            "password":"1234",
            "default_schema":{"$ref":"#/schemas/sjd/name"},
            "prefix_objects":"sjd",
            "allow_ddl":true
        },
        "oracle_consulta2_jumbo_sjd":{
            "name":"oracle_consulta2_jumbo_sjd",
            "driver":{"$ref":"#/drivers/oracle"},
            "host":{"$ref":"#/hosts/winthor"},
            "port":1521,
            "user":"consulta2",
            "password":"consulta2",
            "default_schema":{"$ref":"#/schemas/consulta2"},
            "prefix_objects":"sjd",
            "service":"wint",
            "allow_ddl":true,
            "scripts_iniciais":[
                "ALTER SESSION ENABLE PARALLEL DDL",
                "ALTER SESSION SET DDL_LOCK_TIMEOUT= 600",
                "ALTER SESSION SET NLS_TERRITORY='BRAZIL'",
                "ALTER SESSION SET NLS_DATE_FORMAT='DD/MM/YYYY'",
                "ALTER SESSION SET NLS_LANGUAGE='BRAZILIAN PORTUGUESE'",
                "ALTER SESSION SET NLS_SORT='BINARY'"
            ]
        },
        "oracle_consulta_jumbo_sjd":{
            "name":"oracle_consulta_jumbo_sjd",
            "driver":{"$ref":"#/drivers/oracle"},
            "host":{"$ref":"#/hosts/winthor"},
            "port":1521,
            "user":"consulta",
            "password":"consulta",
            "default_schema":{"$ref":"#/schemas/consulta"},
            "prefix_objects":"sjd",
            "service":"wint",
            "allow_ddl":true,
            "scripts_iniciais":[
                "ALTER SESSION ENABLE PARALLEL DDL",
                "ALTER SESSION SET DDL_LOCK_TIMEOUT= 600",
                "ALTER SESSION SET NLS_TERRITORY='BRAZIL'",
                "ALTER SESSION SET NLS_DATE_FORMAT='DD/MM/YYYY'",
                "ALTER SESSION SET NLS_LANGUAGE='BRAZILIAN PORTUGUESE'",
                "ALTER SESSION SET NLS_SORT='BINARY'"
            ]
        },
        "oracle_jumbo_sjd":{
            "name":"oracle_jumbo_sjd",
            "driver":{"$ref":"#/drivers/oracle"},
            "host":{"$ref":"#/hosts/winthor"},
            "port":1521,
            "user":"jumbo",
            "password":"jumbo",
            "default_schema":{"$ref":"#/schemas/jumbo"},
            "prefix_objects":"",
            "service":"wint",
            "allow_ddl":false
        }
    }    
}